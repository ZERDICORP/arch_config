! appearance !
!!!!!!!!!!!!!!

*background: #111a1f
*foreground: #eff0eb
*cursorColor: #9aedfe

*color0: #33303b
*color8: #4f4b58
*color1: #ff5c57
*color9: #ff5c57
*color2: #5af78e
*color10: #5af78e
*color3: #f3f99d
*color11: #f3f99d
*color4: #57c7ff
*color12: #57c7ff
*color5: #ff6ac1
*color13: #ff6ac1
*color6: #9aedfe
*color14: #9aedfe
*color7: #eff0eb
*color15: #eff0eb

*scrollBar: false

*font: xft:Hack Nerd Font Mono:size=8
*internalBorder: 10
*letterSpace: 0
*antialias: true



!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! perl plugins (sudo pacman -S urxvt-perls) !
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

URxvt.perl-ext-common: default,clipboard,keyboard-select



!!!!!!!!!!!!!!!
! keybindings !
!!!!!!!!!!!!!!!

URxvt.keysym.M-Escape: perl:keyboard-select:activate

URxvt.keysym.Control-p: eval:paste_clipboard

URxvt.keysym.Control-Up: command:\033]720;1\007
URxvt.keysym.Control-Down: command:\033]721;1\007
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C



!!!!!!!!!!!!!!!!!!
! other settings !
!!!!!!!!!!!!!!!!!!

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.clipboard.autocopy: true
